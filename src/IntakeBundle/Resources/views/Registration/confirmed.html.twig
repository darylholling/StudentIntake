{% extends '@Intake/base.html.twig' %}

{% trans_default_domain 'FOSUserBundle' %}

{% block body %}
    <h1 class="text-center topspace">{{ 'registration.confirmed'|trans({'%username%': user.username}) }}</h1>
    {% if targetUrl %}
        <p><a href="{{ targetUrl }}">{{ 'registration.back'|trans }}</a></p>
    {% endif %}

{% endblock %}
